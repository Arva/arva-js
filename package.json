{
  "name": "arva-js",
  "version": "1.1.0",
  "license": "NPOSL-3.0",
  "author": "Tom Clement <tom@bizboard.nl>, Hans van den Akker <hans@bizboard.nl>",
  "description": "An ES6/7 MV* framework for creating highly animated applications for Mobile/Desktop/Web/SmartTV in JavaScript",
  "main": "src/main.js",
  "homepage": "http://arva.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/bizboard/arva-js.git"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive --timeout 15000"
  },
  "bugs": {
    "url": "https://github.com/bizboard/arva-js/issues"
  },
  "jspm": {
    "name": "app",
    "main": "app.js",
    "dependencies": {
      "bowser": "npm:bowser@^1.4.6",
      "camelcase": "npm:camelcase@^3.0.0",
      "canvas": "npm:lodash@^4.16.4",
      "eventemitter3": "npm:eventemitter3@^2.0.2",
      "famous": "github:bizboard/famous@^0.3.7",
      "famous-flex": "github:bizboard/famous-flex@master",
      "fastclick": "npm:fastclick@^1.0.6",
      "firebase": "github:firebase/firebase-bower@^3.0.5",
      "lodash": "npm:lodash@^4.16.4",
      "ordered-hashmap": "npm:ordered-hashmap@^1.0.0",
      "xml2js": "npm:xml2js@^0.4.17"
    },
    "devDependencies": {
      "babel-plugin-transform-async-to-generator": "npm:babel-plugin-transform-async-to-generator@^6.16.0",
      "babel-plugin-transform-builtin-extend": "npm:babel-plugin-transform-builtin-extend@^1.1.0",
      "babel-plugin-transform-class-properties": "npm:babel-plugin-transform-class-properties@^6.16.0",
      "babel-plugin-transform-decorators-legacy": "npm:babel-plugin-transform-decorators-legacy@^1.3.4",
      "babel-plugin-transform-es2015-destructuring": "npm:babel-plugin-transform-es2015-destructuring@^6.16.0",
      "babel-plugin-transform-es2015-for-of": "npm:babel-plugin-transform-es2015-for-of@^6.8.0",
      "babel-plugin-transform-es2015-spread": "npm:babel-plugin-transform-es2015-spread@^6.8.0",
      "babel-plugin-transform-object-rest-spread": "npm:babel-plugin-transform-object-rest-spread@^6.16.0",
      "babel-plugin-transform-regenerator": "npm:babel-plugin-transform-regenerator@^6.16.1",
      "babel-polyfill": "npm:babel-polyfill@^6.16.0",
      "babel-preset-es2015": "npm:babel-preset-es2015@^6.16.0",
      "bcrypt-pbkdf": "npm:bcrypt-pbkdf@^1.0.0",
      "chai": "npm:chai@^3.5.0",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "commander": "npm:commander@^2.9.0",
      "css": "github:systemjs/plugin-css@^0.1.31",
      "dgram": "github:jspm/nodelibs-dgram@^0.2.0-alpha",
      "dns": "github:jspm/nodelibs-dns@^0.2.0-alpha",
      "ecc-jsbn": "npm:ecc-jsbn@~0.1.1",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "jodid25519": "npm:jodid25519@^1.0.0",
      "jsbn": "npm:jsbn@0.1",
      "mocha": "npm:mocha@^3.1.1",
      "mock-browser": "npm:mock-browser@^0.92.12",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
      "options": "npm:options@^0.0.6",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.16",
      "punycode": "github:jspm/nodelibs-punycode@^0.2.0-alpha",
      "querystring": "github:jspm/nodelibs-querystring@^0.2.0-alpha",
      "request-animation-frame-mock": "github:erykpiast/request-animation-frame-mock@^0.1.8",
      "sinon": "npm:sinon@^1.17.6",
      "source-map": "npm:source-map@0.2",
      "systemjs": "npm:systemjs@^0.19.39",
      "tls": "github:jspm/nodelibs-tls@^0.2.0-alpha",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha",
      "tweetnacl": "npm:tweetnacl@0.14",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "timers": "github:jspm/nodelibs-timers@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
    },
    "overrides": {
      "github:erykpiast/request-animation-frame-mock@0.1.8": {
        "dependencies": {
          "extend": "npm:extend"
        }
      },
      "github:firebase/firebase-bower@3.4.1": {
        "main": "firebase.js",
        "registry": "jspm",
        "meta": {
          "firebase.js": {
            "exports": "firebase",
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:esprima@2.7.3": {
        "jspmNodeConversion": false
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.16.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:mocha@3.1.1": {
        "main": "./mocha.js",
        "jspmNodeConversion": false,
        "meta": {
          "mocha.js": {
            "deps": [
              "./mocha.css!"
            ],
            "exports": "Mocha",
            "format": "global"
          }
        },
        "dependencies": {
          "css": "*",
          "debug": "^2.2.0",
          "json3": "npm:json3@3.3.2",
          "lodash.create": "npm:lodash.create@3.1.1"
        },
        "registry": "jspm",
        "map": {
          "./mocha.js": {
            "node": "./lib/mocha.js"
          },
          "escape-string-regexp": "@empty",
          "growl": "@empty",
          "./lib/reporters.js": "./lib/reporters/index.js",
          "./lib/interfaces.js": "./lib/interfaces/index.js",
          "./lib/to-iso-string.js": "./lib/to-iso-string/index.js",
          "glob": "@empty",
          "mkdirp": "@empty",
          "jade": "@empty",
          "diff": "@empty",
          "supports-color": "@empty",
          "path": "@node/path",
          "fs": "@node/fs",
          "util": "@node/util",
          "events": "@node/events",
          "tty": "@node/tty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  },
  "devDependencies": {
    "jspm": "^0.17.0-beta.29",
    "mock-browser": "^0.92.12",
    "systemjs": "^0.19.39"
  }
}
